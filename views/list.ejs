<%- include("header"); %>

<div class="box" id="heading">
    <h1>
        <%= listTitle %>
    </h1>
</div>


<div class="box">
    <% newList.forEach(element => { %>
    <div class="item">
        <input type="checkbox">
        <p>
            <%= element %>
        </p>
    </div>
    <% }); %>


    <form class="item" action="/" method="POST">
        <input type="text" name="item" placeholder="Add a task" aria-autocomplete="off">
        <button type="submit" name="list" value="<%= listTitle %>"> + </button>
    </form>




</div>

<%- include("footer"); %>